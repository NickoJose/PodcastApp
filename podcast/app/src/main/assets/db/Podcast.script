CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE CHANNELS(TITLE VARCHAR(100) NOT NULL PRIMARY KEY,DESC VARCHAR(100),URL VARCHAR(100),PUBLISHDATE VARCHAR(100),AUTHOR VARCHAR(100),CATEGORY VARCHAR(100),OWNER VARCHAR(100),OWNEREMAIL VARCHAR(100), IMAGE VARCHAR(100))
CREATE MEMORY TABLE EPISODES(TITLE VARCHAR(100) NOT NULL PRIMARY KEY,URL VARCHAR(100),DESC VARCHAR(100),LENGTH INTEGER,CH VARCHAR(100),PUBLISHDATE VARCHAR(100),AUTHOR VARCHAR(100),CATEGORY VARCHAR(100),EPNUM INTEGER, IMAGE VARCHAR(100))
CREATE MEMORY TABLE PLAYLISTS(NAME VARCHAR(100) NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE SUBSCRIPTIONS(TITLE VARCHAR(100) NOT NULL PRIMARY KEY)
CREATE MEMORY TABLE CHANNELLIST(TITLE VARCHAR(100) NOT NULL,NAME VARCHAR(100) NOT NULL,CONSTRAINT CONSTRAINT1 PRIMARY KEY(TITLE,NAME),CONSTRAINT CONSTRAINT2 FOREIGN KEY(TITLE) REFERENCES CHANNELS(TITLE),CONSTRAINT CONSTRAINT3 FOREIGN KEY(NAME) REFERENCES PLAYLISTS(NAME))
CREATE MEMORY TABLE EPISODELIST(TITLE VARCHAR(100) NOT NULL,NAME VARCHAR(100) NOT NULL,CONSTRAINT CONSTRAINT4 PRIMARY KEY(TITLE,NAME),CONSTRAINT CONSTRAINT5 FOREIGN KEY(TITLE) REFERENCES EPISODES(TITLE),CONSTRAINT CONSTRAINT6 FOREIGN KEY(NAME) REFERENCES PLAYLISTS(NAME))
CREATE USER SA PASSWORD ""
GRANT DBA TO SA
SET WRITE_DELAY 20
SET SCHEMA PUBLIC
INSERT INTO CHANNELS VALUES('NBA Hang Time','Veteran NBA writer Sekou Smith and former player Greg Anthony analyze the latest NBA news, storylines, and more with guests from around the NBA world.','http://www.nba.com/podcast#/','2016 2 17','Sekou Smith','Sports','NBA Digital','@NBA','nbahangtime')
INSERT INTO CHANNELS VALUES('The Joe Rogan Experience','The Joe Rogan Experience podcast is a long form conversation hosted by comedian, UFC color commentator, and actor Joe Rogan with friends and guests that have included comedians, actors, musicians, MMA instructors and commentators, authors, artists, and porn stars.','http://joerogan.net/podcasts/','2009 12 24','Joe Rogan','Comedy','Brian Redban','BOOKDEATHSQUAD@GMAIL.COM','joerogan')
INSERT INTO CHANNELS VALUES('You Are Not So Smart','You Are Not So Smart is a celebration of self delusion that explores topics related to cognitive biases, heuristics, and logical fallacies. David McRaney interviews scientists about their research into how the mind works, and then he eats a cookie.','https://youarenotsosmart.com/','2012 4 22','David McRaney','Social Sciences','David McRaney','davidmcraney@gmail.com','yourenotsosmart')
INSERT INTO EPISODES VALUES('#890 - Fight Breakdown','http://traffic.libsyn.com/joeroganexp/p890.mp3?dest-id=19997','Joe sits down with Eddie Bravo & Brendan Schaub to discuss upcoming fights in MMA',13740,'The Joe Rogan Experience','2016 12 27','Joe Rogan','Sports',890,'jre890')
INSERT INTO EPISODES VALUES('#990 - Jamie Foxx','http://traffic.libsyn.com/joeroganexp/p990.mp3?dest-id=19997','Jamie Foxx is an Academy Award winning actor, singer, and comedian. He can currently be seen hosting Beat Shazam on Fox and in the movie Baby Driver in theaters now.',4080,'The Joe Rogan Experience','2017 7 27','Joe Rogan','Comedy',990,'jre990')
INSERT INTO EPISODES VALUES('049 - Rejection - Jia Jiang','http://feeds.soundcloud.com/stream/204471744-youarenotsosmart-049-rejection-jia-jiang.mp3','What if you could give yourself a superpower? That is what Jia Jiang wondered when he began a quest to remove the fear of rejection from his brain and become the risk-taking, adventurous person he always wanted to be.',3240,'You Are Not So Smart','2015 5 8','David McRaney','Social Sciences',48,'yourenotsosmart')
INSERT INTO EPISODES VALUES('055 - Weird People - Steven J. Heine','http://feeds.soundcloud.com/stream/217283672-youarenotsosmart-055-weird-people-steven-j-heine.mp3','JIs psychology too WEIRD? That is what our guest, psychologist Steven J. Heine suggested when he and his colleagues published a paper suggesting that psychology was not the study of the human mind, but the study of one kind of human mind, the sort generated by the kinds of brains that happen to be conveniently located near the places where research is usually conducted - North American college undergraduates.',2880,'You Are Not So Smart','2015 8 1','David McRaney','Social Sciences',55,'yourenotsosmart')
INSERT INTO EPISODES VALUES('096 - Progress','http://feeds.soundcloud.com/stream/309444172-youarenotsosmart-096-progress.mp3','Do we have the power to change the outcome of history? Is progress inevitable? Is it natural? Are we headed somewhere definite, or is change just chaos that seems organized in hindsight? In this episode we explore these questions with University of Chicago historian Ada Palmer.',3900,'You Are Not So Smart','2017 2 25','David McRaney','Social Sciences',96,'yourenotsosmart')
INSERT INTO EPISODES VALUES('2017 NBA Draft: Instant Analysis','http://media.adknit.com/a/f/13/nba-hangtime/rl1o6s.1-1.mp3','Lang Whitaker, John Schuhmann, Scott-Howard Cooper and Trey Kerby recap Thursdays draft.',3600,'NBA Hang Time','2017 6 23','Sekou Smith','Sports',61,'hangtime')
INSERT INTO EPISODES VALUES('JRE MMA Show #10 with Tyron Woodley','http://traffic.libsyn.com/joeroganexp/mmashow010.mp3?dest-id=19997','Joe Rogan sits down with UFC Welterweight Champion Tyron Woodley',6120,'The Joe Rogan Experience','2018 1 12','Joe Rogan','Sports',10,'joerogan')
INSERT INTO EPISODES VALUES('More or Less? Predicting Win Totals For All 30 Nba Teams','http://media.adknit.com/a/f/13/nba-hangtime/htwxqy.1-1.mp3','Which NBA teams will be better this season? Which will be worse? Sekou Smith and Lang Whitaker predict how many games each team will win during the 2016-17 NBA season.',2640,'NBA Hang Time','2016 10 21','Sekou Smith','Sports',15,'nbahangtime')
INSERT INTO EPISODES VALUES('State of the Philadelphia 76ers','http://media.adknit.com/a/f/13/nba-hangtime/xpe2c8.1-1.mp3','Sixers insider Jessica Camerato of NBC Sports Philadelphia joins us to dig deep into the state of the 76ers. We talk about Joel Embiid, Ben Simmons, chemistry, playoff hopes, Jahlil Okafor trade, and much more. Then John Schuhmann calls in with his weekly trivia question.',2220,'NBA Hang Time','2017 12 14','Sekou Smith','Sports',82,'nbahangtime')
